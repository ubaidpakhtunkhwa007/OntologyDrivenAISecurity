        <rdf:type rdf:resource="http://www.semanticweb.org/ubaid/ontologies/2023/5/HES#ModelWeaknesses"/>
        <hes:Has rdf:resource="http://www.semanticweb.org/ubaid/ontologies/2023/5/HES#HighSeverity"/>
        <hes:Description>Code injection can be used to manipulate the execution of AI algorithms, potentially altering the behavior of AI models or exposing sensitive information.</hes:Description>
        <hes:Name>Improper Control of Generation of Code (&apos;Code Injection&apos;)</hes:Name>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/ubaid/ontologies/2023/5/HES#CWE-940 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/ubaid/ontologies/2023/5/HES#CWE-940">
