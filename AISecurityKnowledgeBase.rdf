    </owl:Class>
    


    <!-- http://www.semanticweb.org/ubaid/ontologies/2023/5/HES#2.DataPreparation -->

    <owl:Class rdf:about="http://www.semanticweb.org/ubaid/ontologies/2023/5/HES#2.DataPreparation">
